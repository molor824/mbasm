; u32 fib(u32 n)
; r0 = n
; rjl = PC + 4
fib:
    add rsp, rsp, -8
    str.32 rsp[8], rjl

    ; u32 a = 0, b = 1
    ld r1, 0
    ld r2, 1

    ; while (n > 0)
    LOOP0:
        cmp r0, 0
        br.le END0

        ; u32 c = a + b
        add r3, r1, r2
        ; u32 a = b
        mv r1, r2
        ; u32 b = c
        mv r2, r3
        ; n -= 1
        add r0, r0, -1

        jmp LOOP0
    END0:

    ; return a
    mv r0, r1

    add rsp, rsp, 8
    ret rsp[8]

main:
    add rsp, rsp, -8

    ; int i = 1, n = 100
    str.32 rsp[8], 100
    str.32 rsp[4], 1
    ; while (i <= n)
    LOOP1:
        ld.32 r1, rsp[8]
        ld.32 r0, rsp[4]
        cmp r0, r1
        br.gt END1

        ; fib(i)
        call fib
        mv r8, r0

        ; i += 1
        ld.32 r0, rsp[4]
        add r0, 1
        str.32 rsp[4], r0

        jmp LOOP1
    END1:

    halt